            int amountPictures = 52;
            int picturesInRow = 3;
            int totalRowPictures;
            int supermeasurePictures;

            totalRowPictures = amountPictures / picturesInRow;
            supermeasurePictures = amountPictures % picturesInRow;
            Console.WriteLine($"Всего рядов из картинок {totalRowPictures}, сверхнорма составляет {supermeasurePictures}");
