            int growthNumber = 7;

            for (int start = 5; start <= 96; start += growthNumber)
            {
                Console.WriteLine(start);
            }
            // выбор лег на for по причине того что здесь в условии сразу всё указывается,
            // а также в отличие от while здесь всё работает и в console.WriteLine(start) выводится именно уже измененное значение,
            // а в while почему-то не выводилось, только число "5" было и все 
        }
