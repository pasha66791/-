            int age = 52;
            int money = 3;
            int temporaryAge;
            Console.WriteLine("Возраст указан неверно:" + age);
            Console.WriteLine("Количество денег неверное:" + money);
            temporaryAge = money;
            money = age;
            age = temporaryAge;
            Console.WriteLine("Возраст указан верно:" + age);
            Console.WriteLine("Количество денег верное:" + money);
